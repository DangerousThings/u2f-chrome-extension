{
  "name": "FIDO U2F (Universal 2nd Factor) extension",
  "description": "Provides the FIDO U2F APIs for authentication. Pre-release, intended for testing by developers creating U2F servers or devices.",
  "version": "0.8.55",
  "key": "",
  "manifest_version": 2,
  "minimum_chrome_version": "36.0.1985.18",
  "permissions": [
    "usb",
    "hid",
    "https://*/*",
    "http://*/*",
    {
      "usbDevices": [
        { "vendorId": 4176, "productId": 529 },  // Yubico winUSB
        { "vendorId": 4176, "productId": 512 },  // Yubico HID
        { "vendorId": 9601, "productId": 61904 }   // PlugUp
      ]
    }
  ],
  "externally_connectable": {
    "matches": [
      "https://crxjs-dot-u2fdemo.appspot.com/*"
    ],
    "accepts_tls_channel_id": true
  },
  "background": {
    "persistent": false,
    "scripts": [
      "util.js",
      "b64.js",
      "closeable.js",
      "countdown.js",
      "countdowntimer.js",
      "sha256.js",
      "gnubbydevice.js",
      "usbgnubbydevice.js",
      "gnubbies.js",
      "gnubby.js",
      "gnubby-u2f.js",
      "errorcodes.js",
      "gnubbycodetypes.js",
      "devicestatuscodes.js",
      "webrequest.js",
      "gnubbyfactory.js",
      "gnubbymsgtypes.js",
      "messagetypes.js",
      "enroller.js",
      "requestqueue.js",
      "signer.js",
      "requesthelper.js",
      "singlesigner.js",
      "multiplesigner.js",
      "generichelper.js",
      "inherits.js",
      "usbhelper.js",
      "usbenrollhandler.js",
      "usbsignhandler.js",
      "textfetcher.js",
      "appid.js",
      "hidgnubbydevice.js",
      "usbgnubbyfactory.js",
      "delegatinghelper.js",
      "externalhelper.js",
      "helperwhitelist.js",
      "u2fbackground.js"
    ]
  },
  "web_accessible_resources": [
    "u2f-api.js",
    "u2f-comms.js",
    "u2f-comms.html"
  ],
  "icons": {
    "16": "gnubby-16.ico",
    "32": "gnubby-32.ico",
    "48": "gnubby-48.ico",
    "128": "gnubby-128.ico"
  }
}
